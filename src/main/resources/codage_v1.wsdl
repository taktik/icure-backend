    <s4:binding style="document" transport="http://schemas.xmlsoap.org/soap/http" />
    <s1:operation name="encode">
      <s4:operation soapAction="http://www.ehealth.be/webservices/cod/encode" />
      <s1:input>
        <s4:body use="literal" />
