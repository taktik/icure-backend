      <xs:choice>
        <xs:element name="AuthorisationNr" type="core:String50Type"/>
        <xs:element name="CtiExtendedCode" type="core:CtiExtendedType"/>
        <xs:element name="AtcCode" type="core:String7Type"/>
        <xs:element name="AnyNamePart" type="core:SearchStringType"/>
      </xs:choice>
      <xs:element name="Commercialised" type="xs:boolean" minOccurs="0"/>
      <xs:element name="InSupplyProblem" type="xs:boolean" minOccurs="0"/>
      <xs:element name="ComponentEquivalent" type="ComponentEquivalentType" minOccurs="0" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="FindByCompanyType">
    <xs:choice>
      <xs:element name="CompanyActorNr" type="core:CompanyActorNrType"/>
