{
  "name": "icure-cloud",
  "version": "0.0.4",
  "scripts": {
    "dev": "node $NODE_DEBUG_OPTION node_modules/webpack/bin/webpack.js --config webpack.config.js",
    "dev-tz": "node $NODE_DEBUG_OPTION node_modules/webpack/bin/webpack.js --config webpack.config.tz.js",
    "build": "webpack --optimize-minimize --config webpack.config.js",
    "build-tz": "webpack --optimize-minimize --config webpack.config.tz.js",
    "serve": "node --max_old_space_size=4096 node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --progress --port 9000 --open-page \"?icureUrl=http%3A%2F%2F127.0.0.1%3A16043%2Frest%2Fv1&fhcUrl=http%3A%2F%2F127.0.0.1%3A8090#/\" --config webpack.config.js",
    "serve-tz": "node --max_old_space_size=4096 node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --progress --port 9001 --open-page \"?icureUrl=http%3A%2F%2F127.0.0.1%3A16043%2Frest%2Fv1&fhcUrl=http%3A%2F%2F127.0.0.1%3A8090#/\" --config webpack.config.tz.js",
    "publish": "docker build -t docker.taktik.be/icure-frontend:$npm_package_version . && docker push docker.taktik.be/icure-frontend:$npm_package_version",
    "publish-tz": "docker build --build-arg wpc=webpack.config.tz.js --build-arg task=build-tz  --build-arg dist=dist-tz -t docker.taktik.be/topaz-frontend:$npm_package_version . && docker push docker.taktik.be/topaz-frontend:$npm_package_version",
    "webpack-defaults": "webpack-defaults",
    "defaults": "webpack-defaults"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/electron/electron-quick-start.git"
  },
